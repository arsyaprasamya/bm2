<div style="position:absolute;left:82%;top:99.03px" class="cls_004"><span class="cls_004">Jl. BSD Raya Utama No. 63</span></div>
<div style="position:absolute;left:86.5%;top:112.45px" class="cls_004"><span class="cls_004">Tangerang - Banten</span></div>
<div style="position:absolute;left:95.5%;top:125.85px" class="cls_004"><span class="cls_004">15338</span></div>
<div style="position:absolute;left:83.5%;top:139.25px" class="cls_004"><span>Phone: 0818 0669 1710</span></div>
<div style="position:absolute;left:85%;top:152.65px" class="cls_004"><span class="cls_004">www.bresidence.com</span></div>


<p style="position:relative;left:30.83px;top:170.65px" class="p"></p>
<div style="position:absolute;left:5%;top:180px" class="cls_004"><span class="cls_004" style="font-weight: bold;font-size: medium;">Bill To</span></div>


<div style="position:absolute;left:5%;top:205px" class="cls_004"><span class="cls_004" style="font-weight: bold;">Bapak / Ibu <%= billing.billed_to%></span></div>
<div style="position:absolute;left:5%;top:229px" class="cls_004"><span class="cls_004">Unit: <%= billing.unit2%></div>

<div style="position:absolute;left:500px;top:205px" class="cls_004"><span class="cls_004">Invoice No: <%= billing.billing_number %></div>   
<div style="position:absolute;left:500px;top:217px" class="cls_004"><span class="cls_004">Invoice Date: <%= moment(billing.billing_date).format('DD / MM / YYYY') %></span></div>
<div style="position:absolute;left:500px;top:229px" class="cls_004"><span class="cls_004">Payment Due: <%= moment(billing.due_date).format('DD / MM / YYYY') %> </span></div>

<div style="position:absolute;left:5%;top:300px;width:640px;" class="cls_004">
                        <table style="width:100%" class="w3-table-all w3-tiny">
                            <tr class="w3-green">                              
                              <th width="55%" >Description</th>
                              <th width="15%">Usage</th>
                              <th width="15%">Price</th>
                              <th width="15%">Amount</th>
                            </tr>
                            <tr>
                              <td>
                                <p style="font-size: small;font-weight: bold; font-family: Calibri;margin-top: 5px;" >Electricity Consumption</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Pemakaian (<%= detail.electricity.strtpos / 10 %> kWh - <%= detail.electricity.endpos /10 %> kWh )</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">PJU ( <%= detail.electricity.pow.rte.ppju%> % )</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Service Charge ( <%= detail.electricity.pow.rte.srvc%> % )</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Loss ( <%= detail.electricity.loss%> % )</p>
                                
                              </td>
                              <td>&nbsp;
                              <% var endpost = detail.electricity.endpos / 10%>
                              <% var strtpost = detail.electricity.strtpos  / 10%>
                              <% var kwh = endpost - strtpost %>
                              <% var kwh = kwh %>
                              <p style="margin-top: 15px;font-family: Calibri;font-size: 11.1px"><%- kwh %> KWh</p>
                              </td>
                              <td>&nbsp; 
                              <% var rate = detail.electricity.pow.rte.rte %>
                              <p style="margin-top: 14px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0, rate).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                              <p style="margin-top: 65px;font-family: Calibri;font-size: 11.1px"><b>Total</b></p>
                              </td>
                              <td>
                                  <% var amount = detail.electricity.billamnt %>
                                  <% var amountppju = detail.electricity.pow.rte.ppju  * detail.electricity.billamnt / 100  %>
                                  <% var amountforsc = detail.electricity.billamnt + amountppju %>
                                  <% var amountsc = Math.round(detail.electricity.pow.rte.srvc  * amountforsc / 100)  %>
                                  <% var amountaftersc = amountsc + amountforsc  %>
                                  <% var amountloss = detail.electricity.pow.loss  * amountaftersc / 100  %>
                                  <% var amountlosso = detail.electricity.loss  %>
                                  <% var amountElect = amountsc + amountppju + amount %>
                                  <p style="margin-top: 25px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amount).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %> </p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountppju ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountsc ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountlosso ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -2px;font-family: Calibri;font-size: 11.1px"></p><b>Rp. <%= Math.max(0,  amountElect ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.')%>
                              </td>
                            </tr>
                            <tr>
                                <% if("water" in billing.billing)  %>
                                
                                <td>
                                <p style="font-size: small;font-weight: bold; font-family: Calibri;margin-top: 5px;" >Water Consumption</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Total Penggunaan (<%= detail.water.strtpos / 100 %> m<sup>3</sup> - <%= detail.water.endpos / 100 %> m<sup>3</sup> )</p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Administrasi </p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Pemeliharaan </p>
                                <p style="margin-top: -5px;font-family: Calibri; font-size: 11.1px;">Air Kotor ( <%= detail.water.air_kotor%> % )</p>
                                </td>
                                <td>&nbsp;  
                                  <% var endpost = detail.water.endpos / 100 %>
                                  <% var strtpost = detail.water.strtpos / 100 %>
                                  <% var m3 = endpost - strtpost%>
                                  <% var m3 = m3 %>
                                  <p style="margin-top: 15px;font-family: Calibri;font-size: 11.1px"><%= Math.max(0, m3).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %> m<sup>3</sup></p>
                                </td>
                                <td>&nbsp; 
                                  <% var ratew = detail.water.wat.rte.rte %>
                                  <p style="margin-top: 14px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0, ratew).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: 65px;font-family: Calibri;font-size: 11.1px"><b>Total</b></p>
                                </td>
                                <td>&nbsp;
                                    <% var amountW = detail.water.billamnt %>
                                    <% var amountadmin = detail.water.wat.rte.administrasi%>
                                    <% var amountpeml = detail.water.wat.rte.pemeliharaan%>
                                    <% var amountairkotor = detail.water.air_kotor%>
                                    <% var amountWater = amountW + amountadmin + amountpeml %>
                                  <p style="margin-top: 15px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountW).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %> </p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountadmin ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountpeml ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%= Math.max(0,  amountairkotor ).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                  <p style="margin-top: -5px;font-family: Calibri;font-size: 11.1px">Rp. <%- Math.max(0, amountWater).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p>
                                </td>
                            </tr>
                            <tr>  
                                <td><p style="font-family: Calibri;font-size: 11.1px;;">IPL</p></td>
                                <td>
                                  <% var amountIplonly = billing.billing.ipl.amountipl %>
                                  <p>Rp. <%= Math.max(0, amountIplonly).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p> 
                                </td>
                                <td>                                      
                                </td>
                                   
                                <td>
                                  <p> 
                                    <% var amountIplonly = billing.billing.ipl.amountipl %>
                                    <p>Rp. <%= Math.max(0, amountIplonly).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></p> 
                                  </p>                                     
                                </td>
                            </tr>
                            <tr>
                                <td><b>Grand Total</b></td>
                                <td>-</td>
                                <td>-</td>
                                <% gt = amountIplonly + amountWater + amountElect  %>
                                <td><b>Rp. <%= Math.max(0, gt).toFixed(0).replace(/(?=(?:\d{3})+$)(?!^)/g, '.') %></b> </td>
                            </tr>
              </table>
        </div>

